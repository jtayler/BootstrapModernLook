LowerBound: WOTextField {
	numberformat = "#,##0"; //VALID
	size = length; //VALID
	value = minValue;
	class="custom-slider-input";
	name="custom-slider-value-1";
	type="hidden";
}

UpperBound: WOTextField {
	numberformat = "#,##0"; //VALID
	size = length; //VALID
	value = maxValue;
	class="custom-slider-input";
	name="custom-slider-value-2";
	type="hidden";
}

